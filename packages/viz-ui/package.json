{
    "name": "@hkviz/viz-ui",
    "version": "0.0.1",
    "description": "The package containing visualizations of gameplay data",
    "license": "MIT",
    "author": "hkviz",
    "contributors": [],
    "homepage": "https://www.hkviz.org",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hkviz/hkviz-web.git"
    },
    "bugs": {
        "url": "https://github.com/hkviz/hkviz-web/issues"
    },
    "keywords": [
        "hkviz",
        "analytics",
        "solid"
    ],
    "private": true,
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist"
    ],
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typings": "./dist/index.d.cts",
    "exports": {
        "import": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "require": {
            "types": "./dist/index.d.cts",
            "default": "./dist/index.cjs"
        }
    },
    "browser": {},
    "typesVersions": {},
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsup",
        "test": "concurrently pnpm:test:*",
        "test:client": "vitest -c ../../configs/vitest.config.ts",
        "test:ssr": "pnpm run test:client --mode ssr",
        "lint": "eslint --ext .ts,.tsx src",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@hkviz/components": "workspace:*",
        "@hkviz/parser": "workspace:*",
        "@hkviz/viz": "workspace:*",
        "@kobalte/core": "^0.13.1",
        "@preact/signals-react": "^2.0.1",
        "@types/d3": "^7.4.3",
        "d3": "^7.8.5",
        "fuse.js": "^7.0.0",
        "lucide-solid": "^0.378.0",
        "micro-memoize": "^4.1.2"
    },
    "devDependencies": {
        "eslint-plugin-solid": "^0.14.0",
        "solid-js": "^1.8.17"
    },
    "peerDependencies": {
        "solid-js": "^1.8.17"
    }
}
