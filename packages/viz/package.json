{
    "name": "@hkviz/viz",
    "version": "0.0.1",
    "description": "The package containing visualizations of gameplay data",
    "license": "MIT",
    "author": "hkviz",
    "contributors": [],
    "homepage": "https://www.hkviz.org",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hkviz/hkviz-web.git"
    },
    "bugs": {
        "url": "https://github.com/hkviz/hkviz-web/issues"
    },
    "keywords": [
        "hkviz",
        "analytics",
        "solid"
    ],
    "private": true,
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist"
    ],
    "type": "module",
    "main": "./dist/index/index.cjs",
    "module": "./dist/index/index.js",
    "types": "./dist/index/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index/index.d.ts",
                "default": "./dist/index/index.js"
            },
            "require": {
                "types": "./dist/index/index.d.cts",
                "default": "./dist/index/index.cjs"
            }
        },
        "./client-only": {
            "development": {
                "import": {
                    "types": "./dist/client-only/index.d.ts",
                    "default": "./dist/client-only/dev.js"
                },
                "require": {
                    "types": "./dist/client-only/index.d.cts",
                    "default": "./dist/client-only/dev.cjs"
                }
            },
            "import": {
                "types": "./dist/client-only/index.d.ts",
                "default": "./dist/client-only/index.js"
            },
            "require": {
                "types": "./dist/client-only/index.d.cts",
                "default": "./dist/client-only/index.cjs"
            }
        }
    },
    "browser": {},
    "typesVersions": {
        "*": {
            "client-only": [
                "./dist/client-only/index.d.ts"
            ]
        }
    },
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsup",
        "test": "concurrently pnpm:test:*",
        "test:client": "vitest -c ../../configs/vitest.config.ts",
        "test:ssr": "pnpm run test:client --mode ssr",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@hkviz/parser": "workspace:*"
    },
    "devDependencies": {
        "solid-js": "^1.8.17"
    },
    "peerDependencies": {
        "solid-js": "^1.8.17"
    }
}
