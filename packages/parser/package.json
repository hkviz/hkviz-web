{
    "name": "@hkviz/parser",
    "version": "0.0.1",
    "description": "hkviz recording file parser",
    "author": "hkviz",
    "contributors": [],
    "homepage": "https://www.hkviz.org",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hkviz/hkviz-web.git"
    },
    "bugs": {
        "url": "https://github.com/hkviz/hkviz-web/issues"
    },
    "keywords": [
        "hkviz",
        "analytics"
    ],
    "private": true,
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "type": "module",
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "typings": "./dist/index.d.ts",
    "exports": {
        "import": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "require": {
            "types": "./dist/index.d.cts",
            "default": "./dist/index.cjs"
        }
    },
    "typesVersions": {},
    "scripts": {
        "dev": "tsup --watch",
        "dev-old": "pnpm run --filter=. --parallel /^dev:/",
        "dev:tsup": "tsup --watch",
        "dev:tsc": "tsc --watch --emitDeclarationOnly --declaration",
        "build-old": "pnpm run --filter=. --parallel /^build:/",
        "build": "tsup",
        "build:tsup": "tsup",
        "build:tsc": "tsc --emitDeclarationOnly --declaration",
        "test": "concurrently pnpm:test:*",
        "test:client": "vitest -c ../../configs/vitest.config.ts",
        "test:ssr": "pnpm run test:client --mode ssr",
        "tsc-perf": "tsc --noEmit false --declaration --emitDeclarationOnly --extendedDiagnostics --declarationDir ../../dist/debug/declaration/hk-data",
        "tsc-trace": "tsc --generateTrace ./dist/trace && npx @typescript/analyze-trace ./dist/trace"
    },
    "dependencies": {
        "@hkviz/hk-data": "workspace:*",
        "@types/d3": "^7.4.3",
        "d3": "^7.8.5",
        "tsup": "^8.3.5",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "tsup-preset-solid": "^2.2.0",
        "typescript": "^5.4.5"
    },
    "packageManager": "pnpm@8.15.8"
}
