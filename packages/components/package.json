{
    "name": "@hkviz/components",
    "version": "0.0.1",
    "description": "The mostly solid-ui components for hkviz",
    "license": "MIT",
    "author": "hkviz",
    "contributors": [],
    "homepage": "https://www.hkviz.org",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hkviz/hkviz-web.git"
    },
    "bugs": {
        "url": "https://github.com/hkviz/hkviz-web/issues"
    },
    "keywords": [
        "hkviz",
        "components",
        "solid"
    ],
    "private": true,
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist"
    ],
    "type": "module",
    "main": "./dist/server.js",
    "module": "./dist/server.js",
    "types": "./dist/index.d.ts",
    "typings": "./dist/index.d.ts",
    "exports": {
        "worker": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/server.js"
            }
        },
        "browser": {
            "development": {
                "import": {
                    "types": "./dist/index.d.ts",
                    "default": "./dist/dev.js"
                }
            },
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        },
        "deno": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/server.js"
            }
        },
        "node": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/server.js"
            }
        },
        "development": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/dev.js"
            }
        },
        "import": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "browser": {
        "./dist/server.js": "./dist/index.js"
    },
    "typesVersions": {},
    "scripts": {
        "dev": "pnpm run --filter=. --parallel /^dev:/",
        "dev:tsup": "tsup --watch",
        "dev:tsc": "tsc --watch --emitDeclarationOnly --declaration",
        "build": "pnpm run --filter=. --parallel /^build:/",
        "build:tsup": "tsup",
        "build:tsc": "tsc --emitDeclarationOnly --declaration",
        "test": "concurrently pnpm:test:*",
        "test:client": "vitest -c ../../configs/vitest.config.ts",
        "test:ssr": "pnpm run test:client --mode ssr",
        "tsc-perf": "tsc --noEmit false --declaration --emitDeclarationOnly --extendedDiagnostics --declarationDir ../../dist/debug/declaration/hk-data",
        "tsc-trace": "tsc --generateTrace ./dist/trace && npx @typescript/analyze-trace ./dist/trace"
    },
    "dependencies": {
        "@kobalte/core": "^0.13.1",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.0.0",
        "corvu": "^0.5.1",
        "tailwind-merge": "^2.2.0",
        "typescript": "^5.4.5"
    },
    "devDependencies": {
        "solid-js": "^1.8.17"
    },
    "peerDependencies": {
        "solid-js": "^1.8.17"
    }
}
