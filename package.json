{
    "name": "hkviz-web",
    "version": "0.0.1",
    "description": "",
    "scripts": {
        "dev": "pnpm --parallel --reporter append-only --color=always --reporter=append-only run dev",
        "build": "pnpm --recursive --reporter append-only --color=always --reporter=append-only run build",
        "build-pages": "pnpm -r --filter '!app' build && cp ./pnpm-lock.yaml ./packages/app/pnpm-lock.yaml && pnpm -r --filter 'app' build-pages",
        "lint": "pnpm --recursive --reporter append-only --color=always run lint",
        "lint:fix": "pnpm --recursive --reporter append-only --color=always run lint:fix "
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "packageManager": "pnpm@8.15.8",
    "devDependencies": {
        "prettier": "^3.2.5",
        "tsup": "^8.0.2",
        "tsup-preset-solid": "^2.2.0"
    },
    "dependencies": {
        "next": "^14.0.4"
    }
}
